<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/common/">
    <excludes>
        <pattern>/common/*</pattern>
        <pattern>/api/*</pattern>
        <pattern>/login*</pattern>
        <pattern>/error</pattern>
        <pattern>/error/*</pattern>
        <pattern>*/ajax/*</pattern>
        <pattern>*/ajax*</pattern>
    </excludes>
    <decorator name="default" page="default.jsp">
        <pattern>/*</pattern>
    </decorator>
</decorators>
